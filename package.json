{
  "name": "keyknox-sdk",
  "version": "1.0.0",
  "description": "Keyknox SDK",
  "main": "dist/keyknox-sdk.cjs.js",
  "module": "dist/keyknox-sdk.es.js",
  "typings": "dist/types/index.d.ts",
  "repository": "https://github.com/VirgilSecurity/keyknox-javascript",
  "author": "Virgil Security Inc. <support@virgilsecurity.com>",
  "license": "BSD-3-Clause",
  "scripts": {
    "clean": "rimraf .rpt2_cache dist",
    "build:cjs": "cross-env NODE_ENV=cjs rollup -c",
    "build:es": "cross-env NODE_ENV=es rollup -c",
    "build": "npm run build:cjs && npm run build:es",
    "prepare": "npm run clean && npm run build",
    "lint": "tslint --project tsconfig.json",
    "prettier": "prettier 'src/**/*.ts' --write",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "tslint --fix",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "@types/jest": "^23.1.2",
    "@types/node": "^10.5.1",
    "cross-env": "^5.2.0",
    "husky": "^0.14.3",
    "jest": "^23.2.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.6",
    "rimraf": "^2.6.2",
    "rollup": "^0.62.0",
    "rollup-plugin-typescript2": "^0.15.1",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.9.2"
  }
}
